<script type="cdee745108b2ca27a8aa1031-text/javascript">
		$(function() {
			// search bar popover
			$("#search_input").popover();
			// call page specific onLoad
			if (typeof dbipOnLoad === "function") {
				dbipOnLoad.call();
			}
			$(".free_download_link").click(function(ev) {
				if (!$("#license_agree").prop("checked")) {
					alert("You must first read and agree to the licensing terms.");
					ev.preventDefault();
					return false;
				}
			});
			var loadImage = function(els) {
				$.each(els, function(k, el) {
					let $img = $(el.target);
					if ($img.hasClass("lazy") && (el.intersectionRatio > 0)) {
						$img.attr("src", $img.attr("data-src"));
						$img.removeClass("lazy");
					}
				});
			};
			if ("IntersectionObserver" in window) {
				var imgObserver = new IntersectionObserver(loadImage, {
					root: null,
					rootMargin: "50px",
					threshold: 0
				});
				$("img.lazy").each(function(k, img) {
					imgObserver.observe(img);
				});
			} else {
				$("img.lazy").each(function(k, img) {
					$(img).attr("src", $(this).attr("data-src"));
					$(this).removeAttr("lazy");
				});
			}
			if ((typeof window.orientation === "undefined") && (navigator.userAgent.indexOf('IEMobile') === -1)) {
				$(".odometer-fast").each(function(k, el) {
					var o = new Odometer({ el: el });
				});
			}
		});
		</script>

